<!DOCTYPE html>
<html lang="fr">

<head>
    <!-- Meta -->
    <meta charset="UTF-8">
    <!-- maximum-scale=1.0 is now deprecated -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="author" content="">
    <meta name="description" content="Mb Autos Services s'occupe de vos démarches pour vos vehicules">
    <meta name="keywords" content="mbautos, carte grise, immatriculation, pas cher, haubourdin, en ligne ">
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->

    <title>Mb Autos Services</title>

    <!-- Bootstrap core CSS -->
    <link href="{{ asset('asset/css/bootstrap/bootstrap.min.css') }}" rel="stylesheet">

    <!-- Custom fonts for this template -->
    <link href="{{ asset('asset/css/font-awesome/font-awesome.min.css') }}" rel="stylesheet" type="text/css">

    <!-- Custom styles for this template -->
    <link href="{{ asset('asset/css/style.css') }}" rel="stylesheet">
    {% block stylesheets %}
    {% endblock %}

    <link rel="icon" type="image/x-icon" href="??" />

</head>
<body>
<header id="header">
    {% block header %}
        <!-- Nav -->
        <div class="container-fluid">

            <div id="logo" class="pull-left">
                <h1><a href="#intro" class="scrollto">Logo</a></h1>
                <!-- Uncomment below if you prefer to use an image logo -->
                <!-- <a href="#intro"><img src="img/logo.png" alt="" title="" /></a>-->
            </div>

            <nav id="nav-menu-container">
                <ul class="nav-menu">
                    <li class="menu-active"><a href="{{ path('homepage') }}">Acceuil</a></li>
                    <li class="menu-has-children"><a href="#">Demande de carte grise</a>
                        <ul>
                            <li><a href="{{ path('carRegistrationFR') }}">Demande de carte gris française</a></li>
                            <li><a href="{{ path('carRegistrationUE') }}">Demande de carte grise étrangère</a></li>
                        </ul>
                    </li>
                    <li><a href="{{ path('adressChange') }}">Changement d'adresse</a></li>
                    <li><a href="{{ path('quitus') }}">Quitus fiscal</a></li>
                    <li><a href="{{ path('plate') }}">Plaque immatriculation</a></li>
                </ul>
            </nav><!-- #nav-menu-container -->
        </div>
        <!-- /Nav -->
    {% endblock %}
</header>
<!-- Display all flash message -->
{% if app.flashes is defined %}
    <div class="position-fixed">
        {% for label, messages in app.flashes %}
            {% for message in messages %}
                <div class="alert alert-{{ label }} fade in alert-custom">
                    {{ message|raw }}
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                </div>
            {% endfor %}
        {% endfor %}
    </div>
{% endif %}

{% block body %}
{% endblock %}
<footer>
    {% block footer %}
        <div class="footer" id="footer">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-4 contact">
                        <h3> Contact </h3>
                        <p><i class="fa fa-map-marker"></i> : adresse postal
                            <br>59000, ville</p>
                        <p><i class="fa fa-at"></i> : email@email.com </p>
                        <p><i class="fa fa-phone"></i> : 06.06.06.06.06 </p>
                    </div>
                    <div class="col-sm-4 social">
                        <h3> Suivez-Nous </h3>
                        <p><i class="fa fa-facebook-square"></i></p>
                    </div>
                    <div class="col-sm-4 map">
                        <h3> Localisations </h3>
                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2529.17973249859!2d3.19968791558288!3d50.660923380058!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47c327df2e4be83d%3A0x1c7a92aaf30bc020!2s1+Rue+de+la+Blanchisserie%2C+59510+Hem!5e0!3m2!1sfr!2sfr!4v1535973457219" width="100%" frameborder="0" style="border:0" allowfullscreen></iframe>                    </div>
                </div>
                <div class="row copyrights">
                    <div class="col-sm-3">
                        <p><a href="">Mentions Légales</a></p>
                    </div>
                    <div class="col-sm-3">
                        <p><a href="{{ path('indexAdmin') }}">Espace administrateur</a></p>
                    </div>
                </div>
                <!--/.row-->
            </div>
            <!--/.container-->
        </div>
    {% endblock %}
</footer>


<!-- Bootstrap core JavaScript -->
<script src="{{ asset('asset/js/jquery/jquery.min.js') }}"></script>
<script src="{{ asset('asset/js/bootstrap/bootstrap.bundle.min.js') }}"></script>
<script src="{{ asset('asset/js/superfish/superfish.min.js') }}"></script>

<!-- Custom scripts for this template -->
<script src="{{ asset('asset/js/main.js') }}"></script>

{% block javascripts %}
{% endblock %}
</body>
</html>
